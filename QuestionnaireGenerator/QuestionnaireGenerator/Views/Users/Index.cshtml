@model QuestionnaireGenerator.ViewModels.UserData

<h1>User info of @Model.Id</h1>
<p>User Email : @Model.Email</p>
<h2>User Responses : </h2>
@{
    foreach (var response in Model.Responses)
    {
        <p>@response</p>
    }
    
}
<h2>User Affinities : </h2>
@{
    for (int i = 0; i < Model.Affinities.Count; i++)
    {
        <p>@Affinity.AFINNITYVALUES[i] has @Model.Affinities[i]</p>
    }
}